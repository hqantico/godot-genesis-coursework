var SKY_SPEED = 10
var HILLS_SPEED = 30

# Called when the node enters the scene tree for the first time.
func _ready():
	var image_dimension = 1920*0.35
	SKY.motion_mirroring = Vector2(image_dimension,0)
	Hills.motion_mirroring = Vector2(image_dimension,0)
	pass # Replace with function body.


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	#pass
	SKY.motion_offset.x -= SKY_SPEED * delta
	Hills.motion_offset.x -= HILLS_SPEED * delta
"

